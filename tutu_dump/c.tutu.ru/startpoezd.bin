        <div class="tt-calc">
            <div class="tt-calc__heading">
                <div class="tt-calc-title">Когда начнётся продажа билетов <span class="tt-calc-result__accent">на&nbsp;поезд?</span></div>
                <div class="tt-calc-result">Билеты появятся в&nbsp;продаже <span class="tt-calc-result__accent tt-calc-result__date"></span></div>
            </div>
            <form class="tt-calc-form">
                <div class="tt-calc-form__fieldset">
                    <div class="tt-field">
                        <input type="date" class="tt-field__date-input" />
                        <div class="tt-field__label"><div class="tt-field__label-name">Выберите дату отправления</div><div class="tt-field__error">Заполните поле</div></div>
                        
                    </div>
                </div>
                <button type="submit" class="tt-calc-form__button --tt-disabled">Узнать</button>
            </form>
        </div>
        <script>
            const dateInput = document.querySelector('.tt-field__date-input');
            const dateLabel = document.querySelector('.tt-field__label');
            const dateField = document.querySelector('.tt-field');
            const dateError = document.querySelector('.tt-field__error');
            const dateSubmit = document.querySelector('.tt-calc-form__button');
            const dateForm = document.querySelector('.tt-calc-form');
            const dateResult = document.querySelector('.tt-calc-result__date');
            const dateFormWrap = document.querySelector('.tt-calc');

            function checkOnChange () {
                console.log(dateInput.value);
                if (dateInput.value.length == 0) {
                    dateSubmit.classList.add('--tt-disabled');
                } else {
                    const currentDate = new Date();
                    const inputValueDate = new Date(dateInput.value);
                    if (inputValueDate >= currentDate) {
                        dateSubmit.classList.remove('--tt-disabled');
                        dateField.classList.remove('--tt-error');
                    } else {
                        dateError.textContent = 'Введите дату в будущем';
                        dateSubmit.classList.add('--tt-disabled');
                        dateField.classList.add('--tt-error');
                    }
                }
            }
            function getFormattedDate(date) {
                return date.toLocaleString('ru', {
                    day: 'numeric',
                    month: 'long',
                });
            }

            flatpickr('.tt-field__date-input', {
                "locale": "ru",
                minDate: "today",
                onChange: function(selectedDates, dateStr, instance) {
                    dateField.classList.add('--tt-focus');
                },
                onValueUpdate: function(selectedDates, dateStr, instance) {
                    dateField.classList.add('--tt-focus');
                },
            });
            dateInput.addEventListener('change', checkOnChange);
            dateInput.addEventListener('focusout', checkOnChange);
            dateInput.addEventListener('focusin', function() {
                dateField.classList.add('--tt-focus');
                dateField.classList.remove('--tt-error');
            });
            dateForm.addEventListener('submit', function(e) {
                e.preventDefault();
                const userDate = new Date(dateInput.value);
                const calcDate = new Date(userDate - (89*24*60*60*1000));
                dateField.classList.remove('--tt-error');
                if (dateFormWrap.classList.contains('--tt-done')) {
                    dateFormWrap.classList.remove('--tt-done-again');
                    setTimeout(function() {
                        dateFormWrap.classList.add('--tt-done-again');
                        setTimeout(function() {
                            dateResult.innerHTML = getFormattedDate(calcDate); 
                        }, 500);
                    }, 300);
                } else {
                    dateResult.innerHTML = getFormattedDate(calcDate);
                    dateFormWrap.classList.add('--tt-done');
                }
            });
