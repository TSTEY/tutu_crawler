window.addEventListener('load', () => {
try {
		const sessionIdArr = document.cookie
		.split('; ')
  	.map(c => c.split('='))
  	.find(([key, value]) => key === 'SESSIONID');
    
    const sessionId = sessionIdArr === undefined 
    	? undefined 
      : sessionIdArr[1];
  
		console.log(sessionId)
  } catch (err) {
  	console.log(err)
  }
})
