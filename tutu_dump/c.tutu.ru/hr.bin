<script src="https://cdn4.tu-tu.ru/static/webflow/js/smooothy.min.657e06358739d3b976590ef22c7185420561709e.js"></script>
<script src="https://cdn4.tu-tu.ru/static/webflow/js/handleSliderLinks.07dce3876d808a09a5c59235be6353ad9597c226.js"></script>
<!--<script src="https://cdn4.tu-tu.ru/static/webflow/js/handleSliderControls.bee355caf7450d883b2f010a405e132cdf92f134.js"></script>-->
<script>
	let allCardsCount = document.querySelector('[data-numeric="template"]').content.querySelectorAll('.w-dyn-item').length;
  document.querySelector('[data-numeric="count"]').textContent = '(' + allCardsCount + ')';

	
  const jobCardsSlider = new Smooothy(document.querySelector("[data-slider='jobCards']"), {
  	infinite: false,
  });
  const dirsSlider = new Smooothy(document.querySelector("[data-slider='dirsSlider']"), {
  	infinite: false,
  });
  
  function animate() {
    jobCardsSlider.update()
    dirsSlider.update();
    requestAnimationFrame(animate)
  }

  animate();
  handleSliderLinks();
  
  
  function handleSliderControls(slider) {
    const sliderId = slider.wrapper.getAttribute('data-slider');
    const dots = document.querySelector('[data-slider-dots="'+ sliderId +'"]');
    const arrows = document.querySelector('[data-slider-arrows="'+ sliderId +'"]');

    if (!!arrows) {
        Array.from(arrows.children).forEach(function(arrow, index) {
            arrow.addEventListener('click', function(){
            		console.log(index, slider);
                index === 0 ? slider.goToPrev() : slider.goToNext();
            });
        });
    }
    if (!!dots) {
        Array.from(dots.children).forEach(function(dot, index) {
            dot.addEventListener('click', function(){
                slider.goToIndex(index);
            });
        });

        slider.onSlideChange = (current, previous) => {
            Array.from(dots.children)[previous].classList.remove("is-active")
            Array.from(dots.children)[current].classList.add("is-active")
          }
    }
}
  
	handleSliderControls(dirsSlider);
</script>
