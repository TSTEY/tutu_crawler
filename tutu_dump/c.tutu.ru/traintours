document.addEventListener('DOMContentLoaded', function(){
	wrgsv.init('new_id_box','moscow@rzdtour.com,zhdtour@tutu.ru');
  
    const params = new URLSearchParams(document.location.search);
		const urlID = params.get("id");
    if (urlID) {
        setTimeout(function(){
