<script>


		const pageURI = window.location.href;
    console.log(pageURI);

		function getCardInfo(cardElem) {
        	const tourId = cardElem.id;
          const nearestDate = cardElem.querySelector('.rzd_nearest_date').textContent;
          const duration = cardElem.querySelector('.rzd_duration').textContent;
          const tourName = cardElem.querySelector('.rzd_h5').textContent;
          const priceInfo = cardElem.querySelector('.rzd_price').textContent.trim().split('\n');
          const cardsNamesAvailable = [nearestDate, duration, tourName, priceInfo[0], priceInfo[1].trim()];
          return cardsNamesAvailable;
    }

    document.addEventListener('click', function(e) {
        const moreBtn = e.target.closest('#t-show-more');
        const tourCard = e.target.closest('.rzd_card');
        if (moreBtn) {
            e.preventDefault();
            document.querySelector('.t-rzhd-widget').classList.add('--show-all');
        }
        if (tourCard) {
        	console.log(getCardInfo(tourCard));
        }
    });
    
    
    document.querySelectorAll('.t-hero-slider .t-button').forEach(function(sliderButton) {
      sliderButton.addEventListener('click', function(e) {
        e.preventDefault();
        const tourCard = document.getElementById(sliderButton.id.substring(9));
        tourCard.dispatchEvent(new Event('click'));
      });
    });
    
    document.addEventListener('change', function(e) {
    		const programTab = document.getElementById('rzd_tabBtn-1');
        const priceTab = document.getElementById('rzd_tabBtn-2');
        if (!!programTab && programTab.checked) {
        		const tourName = programTab.closest('.rzd_modal').querySelector('.rzd_headTourName').textContent;
            console.log(tourName, 'program checked');
            if (typeof dataLayer !== 'undefined') {
              dataLayer.push({
                'tourProgramClicked': tourName,
                'event': 'gtm.click'
              });
            }
        }
        if (!!priceTab && priceTab.checked) {
        		const tourName = priceTab.closest('.rzd_modal').querySelector('.rzd_headTourName').textContent;
            console.log(tourName, 'price checked');
            if (typeof dataLayer !== 'undefined') {
              dataLayer.push({
                'tourPricesChecked': tourName,
                'event': 'gtm.click'
              });
            }
        }
    });
    
    document.addEventListener('click', function(e) {
        const sendBtn = e.target.closest('#rzd_form button');
        if (!!sendBtn) {
        		const tourName = sendBtn.closest('.rzd_modal').querySelector('.rzd_headTourName').textContent;
            console.log(tourName, 'send clicked');
            if (typeof dataLayer !== 'undefined') {
              dataLayer.push({
                'tourContactSended': tourName,
                'event': 'gtm.click'
              });
            }
        }
    });
</script>
