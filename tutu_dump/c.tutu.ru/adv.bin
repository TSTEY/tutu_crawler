https://cdn.jsdelivr.net/npm/@splidejs/splide@4.1.4/dist/js/splide.min.js
"></script>
<script>
const specSlider = new Splide( document.querySelector('.t-slider_one-col'), {
  // Desktop on down
	perPage: 1,
	perMove: 1,
  focus: 0, // 0 = left and 'center' = center
  type: 'slide', // 'loop' or 'slide'
  arrows: false,
  pagination: false, // 'slider' or false
  autoplay: false,
  speed : 500, // transition speed in miliseconds
  interval: 7000,
  dragAngleThreshold: 60, // default is 30
  autoWidth: false, // for cards with differing widths
  rewind : true, // go back to beginning when reach end
  rewindSpeed : 500,
  waitForTransition : false,
  updateOnMove : true,
  trimSpace: true, // true removes empty space from end of list
  pauseOnHover: false,
  pauseOnFocus: false,
  gap: 32,
  breakpoints: {
  	480: {
			gap: 16,
		},
	}
} );
const sliderParent = document.querySelector('.t-slider__w-text-pages');
const pagesDecor = sliderParent.querySelector('.t-slider__pages-decor');
const sliderPages = sliderParent.querySelectorAll('.t-slider__text-page');
specSlider.mount();
function activatePage(index) {
	const currentPage = sliderPages[index];
  if (window.innerWidth > 479) {
  	pagesDecor.style.left = currentPage.scrollWidth * index + 2 + 'px';
  }
  sliderPages.forEach(function(page, index) {
      page.classList.remove('t-slider__text-page_active');
      });
      currentPage.classList.add('t-slider__text-page_active');
}
sliderPages.forEach(function(page, index) {
	page.addEventListener('click', () => {
		specSlider.go( index );
	})
});
specSlider.on( 'active', function () {
	activatePage(specSlider.index);
} );
const Autoplay = specSlider.Components.Autoplay;
const observerTarget = document.querySelector(".t-slider_one-col");
let options = {
  root: null,
  rootMargin: "0px",
  threshold: 1.0,
};
function callback(entries) {
entries.forEach(function (entry) {
    if (entry.intersectionRatio > 0) {
        Autoplay.play();
        activatePage(0);
        observer.unobserve(observerTarget);
    }
  });
}
const observer = new IntersectionObserver(callback, options);
observer.observe(observerTarget);
//начало кода формы
const switchParent = document.querySelector('.t-radio-switch');
const switchItems = switchParent.querySelectorAll('.t-radio-switch__item');
const switchDecor = switchParent.querySelector('.t-radio-switch__decor');

switchItems.forEach(function(currentItem, currentIndex) {
	currentItem.addEventListener('click', () => {
  	const currentLabel = currentItem.querySelector('.t-radio-switch__label')
  	switchDecor.style.left = currentItem.scrollWidth * currentIndex + 3 + 'px';
  	switchDecor.style.width = currentItem.scrollWidth + 'px';
  	switchItems.forEach(function(item) {
  		const itemLabel = item.querySelector('.t-radio-switch__label')
      itemLabel.classList.remove('t-radio-switch__label_checked');
    });
    currentLabel.classList.add('t-radio-switch__label_checked');
	})
});

const fields = document.querySelectorAll('.t-field');
fields.forEach(function(field) {
	const input = field.querySelector('.t-field__input');
	const label = field.querySelector('.t-field__label');
	field.addEventListener("focusin", (event) => {
  	label.classList.add("t-field__label_focus");
  });
	field.addEventListener("focusout", (event) => {
  	if (input.value.length == 0) {
  		label.classList.remove("t-field__label_focus");
    }
  });
});

class Api {
    async post(formData) {
        const response = await fetch('https://marketing-competition.tutu.ru/adv/register', {
            method: "POST",
            headers: {
                "Content-Type": "application/json",
            },
            body: JSON.stringify(formData),
        });
        const result = await response.json();
        return result;
    }
}
const api = new Api();
const contactForm = document.querySelector('.t-form__form');
const contactFormBlock = document.querySelector('.t-form');
contactForm.onsubmit = function(e) {
    e.preventDefault();
    const formData = {
    	"ad_format": contactForm.adFormat.value,
      /*"full_name": contactForm.fullName.value,*/
      "company_and_position": contactForm.companyAndPosition.value,
      "email": contactForm.email.value,
      "request_description": contactForm.requestDescription.value
    };
    api.post(formData).then((result) => {
    		console.log(result);
        if (result['status'] == 'success') {
            contactFormBlock.classList.add('t-form_success');
        } else {
            contactFormBlock.classList.add('t-form_error');
        }
    });
};
contactForm.addEventListener('focusin', function() {
    contactForm.classList.remove('t-form_error');
    contactForm.classList.remove('t-form_success');
});
</script></body></html>