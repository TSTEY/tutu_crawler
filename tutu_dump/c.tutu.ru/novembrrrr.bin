document.addEventListener('click', function(e) {
	const dayOpen = e.target.closest('.day-open');
	const closeBtn = e.target.closest('.nova-days__item');
  const dayWindow = e.target.closest('.day-open__window');
  if (!dayOpen || !!dayWindow) return;
  dayOpen.classList.add('is-closing');
  document.body.classList.remove('is-scroll-blocked');
  setTimeout(function() {dayOpen.remove()}, 800);
});
document.addEventListener('click', function(e) {
	const codeBlock = e.target.closest('.day-open__code');
  if (!!codeBlock) {
  	const codeBlockText = codeBlock.querySelector('.day-open__code-text').textContent;
    navigator.clipboard.writeText(codeBlockText).then(
      () => {
        codeBlock.classList.add('is-copied');
  			setTimeout(function() {codeBlock.classList.remove('is-copied');}, 2000);
      },
    );
  }
});

document.addEventListener('click', function(e) {
    const link = e.target.closest('.day-open__save');
    if (link) {
        e.preventDefault();
        let url = link.getAttribute("data-save");
        let fileName = "tutu-advent";
        let xhr = new XMLHttpRequest();
        xhr.open("GET", url, true);
        xhr.responseType = "blob";
        xhr.onload = function(){
            let urlCreator = window.URL || window.webkitURL;
            let imageUrl = urlCreator.createObjectURL(this.response);
            let tag = document.createElement('a');
            tag.href = imageUrl;
            tag.download = fileName;
            document.body.appendChild(tag);
            tag.click();
            document.body.removeChild(tag);
        }
        xhr.send();
    }
});

function closeToForm() {
	const dayOpen = document.querySelector('.day-open');
	const formBlockTop = document.getElementById('adventForm').getClientRects()[0].y;
  dayOpen.classList.add('is-closing');
  document.body.classList.remove('is-scroll-blocked');
  setTimeout(function() {
  	dayOpen.remove();
    window.scrollTo({ top: formBlockTop, behavior: "smooth" });
  }, 800);
}
</script>
<script>
    class Api {
        async post(formData, url) {
            const response = await fetch(url, {
                method: "POST",
                headers: {
                    "Content-Type": "application/json",
                },
                body: JSON.stringify(formData),
            });
            const result = await response.json();
            return result;
        }
    }

    const api = new Api();
  
    const fields = document.querySelectorAll('.t-field');
    document.addEventListener("focusin", (event) => {
        const field = event.target.closest('.t-field');
        if (!field) return;
        const input = field.querySelector('.t-field__input');
        const label = field.querySelector('.t-field__label');
        label.classList.add("t-field__label_focus");
        field.classList.remove('--t-error');
    });
    document.addEventListener("focusout", (event) => {
        const field = event.target.closest('.t-field');
        if (!field) return;
        const input = field.querySelector('.t-field__input');
        const label = field.querySelector('.t-field__label');
        if (input.value.length == 0) {
        label.classList.remove("t-field__label_focus");
        }
    });
    const contactForm = document.querySelector('.t-form__form');
    const contactFormBlock = document.querySelector('.t-form__wrap');

    
    contactForm.onsubmit = function(e) {
        e.preventDefault();
        const isAdvAgreed = contactForm.advCheckbox.checked;
        const fetchURL = isAdvAgreed ? 'https://marketing-endpoints.tutu.ru/tours_promo_adv_2' : 'https://marketing-endpoints.tutu.ru/tours_promo_pd_2';
        const formData = {
            "email": contactForm.email.value,
        };
        api.post(formData, fetchURL).then((result) => {
            
            if (result['status'] === 'success') {
                contactFormBlock.classList.add('t-form_success');
                
            } else if (result['error_code'] === 'duplicate_entry') {
                const errorField = document.getElementById('emailWrap');
                errorField.classList.add('--t-error');
            }  else if (result['error_code'] === 'internal_error') {
                contactForm.classList.add('t-form_error');
            } else {
                contactFormBlock.classList.add('t-form_error');
            }
        });
    };
    contactForm.addEventListener('focusin', function() {
        contactForm.classList.remove('t-form_error');
        contactForm.classList.remove('t-form_success');
    });
</script></body></html>